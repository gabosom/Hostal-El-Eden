<body id="page1" onload="new ElementMaxHeight();">
    
    <div id="main">
<!-- header -->
	<div id="header">
		<div class="row-1">
			<div class="wrapper">
				<div class="logo">
					<h1><a href="@Url.Action("Index", "Home")">Hostal El Eden</a></h1>
