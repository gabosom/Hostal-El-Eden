					+(593)-274-0616
				</div>
			</div>
		</div>
		<div class="row-2">
	 		<div class="indent">
<!-- header-box begin -->
				<div class="header-box">
					<div class="inner">
						@Html.Partial("NavMenu")
					</div>
				</div>
<!-- header-box end -->
			</div>
		</div>
	</div><div class="ic">Lovely Flash templates from TemplateMonster!</div>
<!-- content -->
	<div id="content">
		<div class="wrapper">
        @RenderSection("Aside", false)
			
			
			<div class="content">
                <div class="indent">
                    @RenderBody()
                </div>
				
                
			</div>
		</div>
	</div>
<!-- footer -->
	<div id="footer">
  		<ul class="nav">
	 		<li><a href="@Url.Action("Index", "Home")">Inicio</a> |</li>
	<li><a href="@Url.RouteUrl("CategoriesToRooms", new { action = "" })" >Habitaciones</a> |</li>
	<li><a href="@Url.RouteUrl("Gallery")">Galeria</a> |</li>
	<li><a href="@Url.RouteUrl("Restaurant")">Restaurant</a> |</li>
	<li><a href="@Url.RouteUrl("Banos")">Ba&ntilde;os</a> |</li>
	<li><a href="@Url.RouteUrl("Contact")">Contactanos</a></li>
		</ul>
		<div class="wrapper">
			<div class="fleft">Copyright (c) 2009 FiveStar Hotel</div>
			<div class="fright"><a rel="nofollow" href="http://www.templatemonster.com/flash-templates.php">Flash Templates</a> from TemplateMonster - Squeeze Your Competitors.</div>
		</div>
	</div>
</div>

